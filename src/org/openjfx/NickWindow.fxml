<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="nickWindow" prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.NickController">
   <children>
      <TextField fx:id="nick" layoutX="7.0" layoutY="44.0" onKeyTyped="#writenick" />
      <Label layoutX="63.0" layoutY="14.0" text="Podaj Nick:" />
      <Button fx:id="okej" layoutX="31.0" layoutY="97.0" mnemonicParsing="false" onAction="#play" text="Ok" />
      <Button fx:id="anuluj" layoutX="114.0" layoutY="97.0" mnemonicParsing="false" onAction="#cancel" text="Anuluj" />
   </children>
</AnchorPane>
